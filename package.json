{
  "name": "tickets",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "bun@1.3.6",
  "imports": {
    "#fp": "./src/fp/index.ts",
    "#fp/*": "./src/fp/*",
    "#test-utils": "./src/test-utils/index.ts",
    "#test-utils/*": "./src/test-utils/*",
    "#jsx/*": "./src/lib/jsx/*",
    "#lib/db/*": "./src/lib/db/*",
    "#lib/rest/*": "./src/lib/rest/*",
    "#lib/jsx/*": "./src/lib/jsx/*",
    "#lib/*": "./src/lib/*",
    "#routes": "./src/routes/index.ts",
    "#routes/*": "./src/routes/*",
    "#templates/*": "./src/templates/*",
    "#src/*": "./src/*",
    "#test/*": "./test/*"
  },
  "scripts": {
    "start": "bun src/index.ts",
    "build:edge": "bun scripts/build-edge.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "bunx @biomejs/biome check --error-on-warnings src test scripts",
    "lint:fix": "bunx @biomejs/biome check --write src test scripts",
    "cpd": "bunx jscpd src --config .jscpd.json",
    "typecheck": "bunx tsc --noEmit",
    "precommit": "bun run typecheck && bun run lint && bun run cpd && bun run test:coverage"
  },
  "dependencies": {
    "@noble/ciphers": "^2.1.1",
    "@noble/hashes": "^2.0.1",
    "stripe": "^17.0.0"
  },
  "devDependencies": {
    "@bunny.net/edgescript-sdk": "^0.12.0",
    "@libsql/client": "^0.17.0",
    "@types/bun": "^1.1.0",
    "jscpd": "^4.0.5",
    "typescript": "^5.8.0"
  }
}
