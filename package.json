{
  "name": "test-searcher",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "bun@1.3.6",
  "imports": {
    "#fp": "./chobble-template/packages/js-toolkit/fp/index.js",
    "#fp/*": "./chobble-template/packages/js-toolkit/fp/*",
    "#test-utils": "./chobble-template/packages/js-toolkit/test-utils/index.js",
    "#test-utils/*": "./chobble-template/packages/js-toolkit/test-utils/*",
    "#src/*": "./src/*",
    "#test/*": "./test/*",
    "#lib/*": "./src/lib/*"
  },
  "scripts": {
    "build:edge": "bun scripts/build-edge.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "bunx @biomejs/biome check --error-on-warnings src test scripts",
    "lint:fix": "bunx @biomejs/biome check --write src test scripts",
    "cpd": "bunx jscpd src --config .jscpd.json",
    "typecheck": "bunx tsc --noEmit",
    "precommit": "bun run typecheck && bun run lint && bun run cpd && bun run test:coverage"
  },
  "devDependencies": {
    "@bunny.net/edgescript-sdk": "^0.12.0",
    "@libsql/client": "^0.17.0",
    "@types/bun": "^1.1.0",
    "jscpd": "^4.0.5",
    "typescript": "^5.8.0"
  },
  "dependencies": {
    "node-html-parser": "^7.0.2",
    "unpdf": "^0.12.1"
  }
}
